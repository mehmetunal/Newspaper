<?xml version="1.0" encoding="utf-8"?>
<configuration>
  <packageSources>
    <add key="nuget.org" value="https://api.nuget.org/v3/index.json" protocolVersion="3" />
    <add key="maggsoft" value="https://nuget.pkg.github.com/mehmetunal/index.json" />
  </packageSources>
  <packageSourceCredentials>
    <maggsoft>
      <add key="Username" value="mehmetunal" />
      <add key="ClearTextPassword" value="%GITHUB_TOKEN%" />
    </maggsoft>
  </packageSourceCredentials>
  <packageSourceMapping>
    <packageSource key="nuget.org">
      <package pattern="*" />
    </packageSource>
    <packageSource key="maggsoft">
      <package pattern="Maggsoft.*" />
    </packageSource>
  </packageSourceMapping>
  <disabledPackageSources>
    <clear />
  </disabledPackageSources>
</configuration>
